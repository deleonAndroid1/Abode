<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.training.android.abode">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo101"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".SearchApartmentsMaps"
            android:label="@string/title_activity_search_apartments_maps" />
        <activity android:name=".Profile" />
        <activity android:name=".Apartment.HomeActivity" />
        <activity android:name=".ApartmentActivity" />
        <activity android:name=".ViewSearchedApartments" />
        <activity android:name=".Samples.SampleInputToDB" />
        <activity android:name=".Maps.SearchforAparts" />
        <activity android:name=".NoticeBoard"></activity>
    </application>

</manifest>